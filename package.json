{
    "name": "zeus-system",
    "version": "1.0.0",
    "description": "Sistema interno para gestão de membros, clientes, equipamentos e penalidades.",
    "main": "dist/index.js",
    "scripts": {
        "start": "node dist/index.js",
        "dev": "nodemon --watch src --ext ts --exec ts-node src/index.ts",
        "build": "tsc",
        "prestart": "npm run build",
        "lint": "eslint . --ext .ts --fix",
        "format": "prettier --write src/**/*.ts",
        "test": "jest --watchAll --coverage",
        "typeorm": "ts-node ./node_modules/typeorm/cli.js",
        "migration:generate": "npm run typeorm migration:generate src/migrations/$npm_config_name -d src/database/data-source.ts",
        "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run -d src/database/data-source.ts",
        "migration:revert": "npm run typeorm migration:revert -d src/database/data-source.ts",
        "db:migrate": "typeorm-ts-node-commonjs migration:run -d \"src/database/data-source.ts\"",
        "stop:dev": "kill-port 4001",
        "swagger-autogen": "swagger-autogen ./swagger-output.json ./src/routes/*.ts",
        "swagger-ui": "swagger-ui-express",
        "start:ngrok": "ngrok http 4001"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/torrescf/PROJETO-ZEUS.git"
    },
    "keywords": [
        "comp-junior",
        "management-system",
        "nodejs",
        "typeorm"
    ],
    "author": "João Pedro Oliveira",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/torrescf/PROJETO-ZEUS/issues"
    },
    "homepage": "https://github.com/torrescf/PROJETO-ZEUS#readme",
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "cloudinary": "^2.6.1",
        "compression": "^1.8.0",
        "cors": "^2.8.5",
        "csurf": "^1.2.2",
        "date-fns": "^2.30.0",
        "dotenv": "^16.5.0",
        "envalid": "^8.0.0",
        "express": "^4.18.2",
        "express-rate-limit": "^6.11.2",
        "express-validator": "^7.2.1",
        "helmet": "^8.1.0",
        "jsonwebtoken": "^9.0.2",
        "nodemailer": "^6.10.1",
        "pg": "^8.15.6",
        "reflect-metadata": "^0.1.14",
        "routing-controllers": "^0.11.2",
        "swagger-ui-express": "^4.6.3",
        "typeorm": "^0.3.23",
        "winston": "^3.17.0",
        "zod": "^3.24.4"
    },
    "devDependencies": {
        "@eslint/css": "^0.7.0",
        "@eslint/js": "^9.26.0",
        "@eslint/json": "^0.12.0",
        "@types/babel__core": "^7.20.5",
        "@types/babel__generator": "^7.27.0",
        "@types/babel__template": "^7.4.4",
        "@types/babel__traverse": "^7.20.7",
        "@types/bcryptjs": "^2.4.2",
        "@types/compression": "^1.7.5",
        "@types/cors": "^2.8.18",
        "@types/express": "^4.17.17",
        "@types/express-rate-limit": "^5.1.3",
        "@types/express-validator": "^2.20.33",
        "@types/helmet": "^0.0.48",
        "@types/istanbul-lib-coverage": "^2.0.6",
        "@types/jest": "^29.5.14",
        "@types/json-schema": "^7.0.15",
        "@types/jsonwebtoken": "^9.0.2",
        "@types/methods": "^1.1.4",
        "@types/multer": "^1.4.12",
        "@types/node": "^20.5.7",
        "@types/nodemailer": "^6.4.17",
        "@types/supertest": "^2.0.16",
        "@types/swagger-ui-express": "^4.1.8",
        "@types/triple-beam": "^1.3.5",
        "@types/winston": "^2.4.4",
        "@types/yargs": "^17.0.33",
        "@types/yargs-parser": "^21.0.3",
        "@typescript-eslint/eslint-plugin": "^6.4.1",
        "@typescript-eslint/parser": "^6.4.1",
        "eslint": "^8.57.1",
        "eslint-config-prettier": "^9.0.0",
        "eslint-plugin-prettier": "^5.0.0",
        "globals": "^16.1.0",
        "jest": "^29.6.4",
        "nodemon": "^3.0.1",
        "prettier": "^3.0.2",
        "supertest": "^6.3.3",
        "ts-jest": "^29.1.1",
        "ts-node-dev": "^2.0.0",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.2.2",
        "typescript-eslint": "^8.32.0"
    },
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=9.0.0"
    }
}
